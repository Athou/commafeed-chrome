{
  "manifest_version": 2,

  "name": "CommaFeed",
  "icons": {
    "128": "icon128.png"
  },
  "description": "CommaFeed Chrome extension",
  "version": "1.4",
  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
